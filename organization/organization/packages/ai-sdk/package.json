{
  "name": "@broxiva/ai-sdk",
  "version": "2.0.0",
  "description": "AI SDK for Broxiva platform - client libraries for AI services",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./recommendation": {
      "types": "./dist/recommendation.d.ts",
      "import": "./dist/recommendation.mjs",
      "require": "./dist/recommendation.js"
    },
    "./search": {
      "types": "./dist/search.d.ts",
      "import": "./dist/search.mjs",
      "require": "./dist/search.js"
    },
    "./personalization": {
      "types": "./dist/personalization.d.ts",
      "import": "./dist/personalization.mjs",
      "require": "./dist/personalization.js"
    },
    "./fraud": {
      "types": "./dist/fraud.d.ts",
      "import": "./dist/fraud.mjs",
      "require": "./dist/fraud.js"
    },
    "./chatbot": {
      "types": "./dist/chatbot.d.ts",
      "import": "./dist/chatbot.mjs",
      "require": "./dist/chatbot.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/recommendation.ts src/search.ts src/personalization.ts src/fraud.ts src/chatbot.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@broxiva/types": "workspace:*",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitest/coverage-istanbul": "^1.0.0",
    "eslint": "^8.57.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "ai",
    "sdk",
    "ml",
    "broxiva"
  ],
  "license": "UNLICENSED"
}
